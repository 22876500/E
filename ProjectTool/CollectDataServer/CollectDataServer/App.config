<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <connectionStrings>
    <add name="strCon" connectionString="Data Source =. \SQLEXPRESS;database=CD;integrated security=SSPI" />
  </connectionStrings>
